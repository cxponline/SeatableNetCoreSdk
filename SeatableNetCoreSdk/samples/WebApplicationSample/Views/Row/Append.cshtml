@model RowAppendViewModel
@{
    ViewData["Title"] = "追加一行";
    var data = ViewData["data"] as Dictionary<string, object>;
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item active" aria-current="page">@ViewData["Title"]</li>
    </ol>
</nav>
<br />
<div class="card">
    <div class="card-body">
        <form asp-controller="Row" asp-action="Append" method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Factory">工厂</label>
                <input type="text" class="form-control" asp-for="Factory" value="" />
            </div>
            <div class="form-group">
                <label asp-for="Product">产品</label>
                <input type="text" class="form-control" asp-for="Product" value="" />
            </div>
            <div class="form-group">
                <label asp-for="Rate">损失率</label>
                <input type="text" class="form-control" asp-for="Rate" value="" />
            </div>
            <div class="form-group">
                <label asp-for="UUID">dtable-uuid</label>
                <input type="text" class="form-control" asp-for="UUID" value="" />
            </div>

            <div class="form-group">
                <label asp-for="TableName">表名</label>
                <input type="text" class="form-control" asp-for="TableName" value="n8n-table" />
            </div>
            <button type="submit" class="btn btn-primary">提交请求</button>  结果:<span>@data["_id"]</span>
        </form>
        <hr />
    </div>
</div>